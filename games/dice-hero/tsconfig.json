{
  "compilerOptions": {
    "sourceMap": true,
    "strict": true,
    "module": "ESNext",
    "target": "esnext",
    "isolatedModules": true,
    "moduleResolution": "Bundler",
    "types": ["vite/client"],
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "skipLibCheck": true,
    "noEmit": true
  }
}
