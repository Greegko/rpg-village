      "default": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "rambda": "9.2.1",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "ts-patch": "3.2.0",
    "typescript": "5.5.2",
    "typescript-transform-paths": "3.4.7"
  }
}
